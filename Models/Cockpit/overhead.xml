<!-- Engine Start & Fuel Jettison -->

<animation>
	<type>pick</type>
	<object-name>auto-ignition</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>  
		<binding>
			<command>property-adjust</command>
			<property>/controls/engines/auto-ignition</property>
			<step>1</step>
			<min>0</min>
			<max>2</max>
		</binding>
	</action>
</animation>

<animation>
	<type>pick</type>
	<object-name>auto-ignition</object-name>
	<action>
		<button>1</button>
		<repeatable>false</repeatable>  
		<binding>
			<command>property-adjust</command>
			<property>/controls/engines/auto-ignition</property>
			<step>-1</step>
			<min>0</min>
			<max>2</max>
		</binding>
	</action>
</animation>

<animation>
	<type>rotate</type>
	<object-name>auto-ignition</object-name>
	<property>/controls/engines/auto-ignition</property>
	<factor>-40</factor>
	<center>
		<x-m> 0.026</x-m>
		<y-m>-0.034</y-m>
		<z-m>-0.014</z-m>
	</center>
	<axis>
		<x>0</x>
		<y>0</y>
		<z>1</z>
	</axis>
</animation>

<animation>
	<type>pick</type>
	<object-name>con-ignition</object-name>
	<object-name>con-ignition_lit</object-name>
	<object-name>con-ignition_unlit</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>  
		<binding>
			<command>property-toggle</command>
			<property>/controls/engines/con-ignition</property>
		</binding>
	</action>
</animation>

 <animation>
  	<type>select</type>
  	<object-name>con-ignition_lit</object-name>
    <condition>
		<equals>
			<property>/controls/engines/con-ignition</property>
			<value>1</value>
		</equals>
    </condition>
 </animation>

 <animation>
  	<type>select</type>
  	<object-name>con-ignition_unlit</object-name>
    <condition>
		<not>
			<equals>
				<property>/controls/engines/con-ignition</property>
				<value>1</value>
			</equals>
		</not>
    </condition>
 </animation>

