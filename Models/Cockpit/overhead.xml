<PropertyList>

<path>overhead.ac</path>

<!-- 2D panels -->

<animation>
	<type>pick</type>
	<object-name>panel.elec</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>  
		<binding>
		  <command>property-assign</command>
		  <property>sim/panel/path</property>
		  <value>Aircraft/747-400/Panels/744_elec_panel.xml</value>
		</binding>
		<binding>
		  <command>property-assign</command>
		  <property>sim/panel/visibility</property>
		  <value>true</value>
		</binding>
		<binding>
		  <command>panel-load</command>
		</binding>
	</action>
</animation>

<animation>
	<type>pick</type>
	<object-name>panel.hyd</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>  
		<binding>
		  <command>property-assign</command>
		  <property>sim/panel/path</property>
		  <value>Aircraft/747-400/Panels/744_hyd_panel.xml</value>
		</binding>
		<binding>
		  <command>property-assign</command>
		  <property>sim/panel/visibility</property>
		  <value>true</value>
		</binding>
		<binding>
		  <command>panel-load</command>
		</binding>
	</action>
</animation>

