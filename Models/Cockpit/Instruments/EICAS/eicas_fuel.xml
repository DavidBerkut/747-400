<path>eicas_fuel.ac</path>

<!-- Total Fuel-->
 <model>
	<path>Aircraft/747-400/Models/Cockpit/display-number.xml</path>
	<offsets>
		<x-m> 0.007</x-m>
		<y-m> 0</y-m>
		<z-m> 0.084</z-m>
	</offsets>
	<overlay>
		<params>
			<property type="string">/fdm/jsbsim/propulsion/total-fuel-lbs</property>
			<format type="string">%03.1f</format>
			<factor>0.001</factor>
			<character-size type="double">0.007</character-size>
			<color>
				<red   type="double">1.0</red>
				<green type="double">1.0</green>
				<blue  type="double">1.0</blue>
			</color>
		</params>
	</overlay>
</model>

