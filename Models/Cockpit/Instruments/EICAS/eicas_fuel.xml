 <path>eicas_fuel.ac</path>

<!-- Total Fuel-->
 <animation>
   <type>select</type>
   <object-name>total.1</object-name>
   <condition>
    <greater-than>
     <property>/fdm/jsbsim/propulsion/total-fuel-lbs</property>
     <value>99995</value>
    </greater-than>
   </condition>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>total.1</object-name>
<property>/fdm/jsbsim/propulsion/total-fuel-lbs</property>
   <factor>0.000001</factor>
   <step>100000.0</step>
