<!-- AUTOBRAKE -->

<animation>
	<type>pick</type>
	<object-name>sw.autobrake</object-name>
	<action>
		<button>0</button>
		<binding>
			<command>property-adjust</command>
			<property>/autopilot/autobrake/step</property>
			<step>1</step>
			<min>-2</min>
			<max>5</max>
		</binding>
	</action>
	<action>
		<button>1</button>
		<binding>
			<command>property-adjust</command>
			<property>/autopilot/autobrake/step</property>
			<step>-1</step>
			<min>-2</min>
			<max>5</max>
		</binding>
	</action>
	<action>
		<button>3</button>
		<binding>
			<command>property-adjust</command>
			<property>/autopilot/autobrake/step</property>
			<step>1</step>
			<min>-2</min>
			<max>5</max>
		</binding>
	</action>
	<action>
		<button>4</button>
		<binding>
			<command>property-adjust</command>
			<property>/autopilot/autobrake/step</property>
			<step>-1</step>
			<min>-2</min>
			<max>5</max>
		</binding>
	</action>
</animation>

<animation>
	<type>rotate</type>
	<object-name>sw.autobrake</object-name>
	<property>/autopilot/autobrake/step</property>
	<interpolation>
		<entry>
			<ind>-2</ind>
			<dep>30</dep>
		</entry>
		<entry>
			<ind>-1</ind>
			<dep>0</dep>
		</entry>
		<entry>
			<ind>0</ind>
			<dep>-30</dep>
		</entry>
		<entry>
			<ind>1</ind>
			<dep>-60.0</dep>
		</entry>
		<entry>
			<ind>2</ind>
			<dep>-90.0</dep>
		</entry>
		<entry>
			<ind>3</ind>
			<dep>-120.0</dep>
		</entry>
		<entry>
			<ind>4</ind>
			<dep>-150.0</dep>
		</entry>
		<entry>
			<ind>5</ind>
			<dep>-180.0</dep>
		</entry>
	</interpolation>
	<center>
		<x-m>1.544</x-m>
		<y-m>0.119</y-m>
		<z-m>0.413</z-m>
	</center>
	<axis>
		<x>0</x>
		<y>0</y>
		<z>1</z>
	</axis>
</animation>

