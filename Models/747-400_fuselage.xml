<PropertyList>

<path>747-400_fuselage.ac</path>

<nasal>
 <load>
            var livery_update = aircraft.livery_update.new("Aircraft/747-400/Models/Liveries", 10,
			func { print("747-400 fuselage livery update") });
 </load>
 <unload>
            livery_update.stop();
 </unload>
</nasal> 

<animation> 
