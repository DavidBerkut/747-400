<PropertyList>

 <path>747-400_fuselage.ac</path>
 <texture-path>Textures/Livery/KLM/</texture-path>
 <offsets>
  <x-m>0</x-m>
  <y-m>0</y-m>
  <z-m>0</z-m>
 </offsets>

<nasal> 
<load> var livery_update = aircraft.livery_update.new("Aircraft/747-400/Models/Liveries"); </load> 
<unload> 
livery_update.stop(); 
</unload> 
</nasal> 

<animation> 
<type>material</type> 
<object-name>Mesh1 Group_49_1 Grou_1</object-name> 
<object-name>Mesh10 Fuselage_000_2_2</object-name> 
<object-name>Mesh11 FuselageAftT_2_2</object-name> 
<object-name>Mesh2 Group_50_1 Grou_1</object-name> 
<object-name>Mesh3 Group_48_1 Grou</object-name> 
<object-name>Mesh4 Group_46_1 Grou</object-name> 
<object-name>Mesh5 Group1 Model_1</object-name> 
<property-base>sim/model/livery</property-base> 
<texture-prop>texture</texture-prop> 
<texture>747-400_l.rgb</texture> 
</animation> 
 
 <!-- DOORS -->
 <!-- LARGE DOORS -->

 <animation>
  <type>rotate</type>
  <object-name>Mesh2 Group_50_1 Grou</object-name>
  <property>gear/gear[0]/position-norm</property>
  <offset-deg>0</offset-deg>
  <interpolation>
    <entry>
     <ind>0.0</ind>
     <dep>0</dep>
    </entry>
    <entry>
     <ind>0.1</ind>
     <dep>-80</dep>
    </entry>
    <entry>
     <ind>0.9</ind>
     <dep>-80</dep>
    </entry>
    <entry>
     <ind>1.0</ind>
     <dep>0</dep>
    </entry>
  </interpolation>
  <axis>
   <x1-m>-26.304</x1-m>
   <y1-m>-0.885</y1-m>
   <z1-m>-1.401</z1-m>
   <x2-m>-23.439</x2-m>
   <y2-m>-0.871</y2-m>
   <z2-m>-1.867</z2-m>
  </axis>
 </animation>


 <animation>
  <type>rotate</type>
  <object-name>Mesh1 Group_49_1 Grou_0</object-name>
  <property>gear/gear[0]/position-norm</property>
  <offset-deg>0</offset-deg>
  <interpolation>
    <entry>
     <ind>0.0</ind>
     <dep>0</dep>
    </entry>
    <entry>
     <ind>0.1</ind>
     <dep>80</dep>
    </entry>
    <entry>
     <ind>0.9</ind>
     <dep>80</dep>
    </entry>
    <entry>
     <ind>1.0</ind>
     <dep>0</dep>
    </entry>
  </interpolation>
  <axis>
   <x1-m>-26.304</x1-m>
   <y1-m>0.885</y1-m>
   <z1-m>-1.401</z1-m>
   <x2-m>-23.439</x2-m>
   <y2-m>0.871</y2-m>
   <z2-m>-1.867</z2-m>
  </axis>
 </animation>

<!-- SMALL DOORS -->

