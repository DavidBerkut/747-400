<PropertyList>

<path>747-400_fuselage.ac</path>

    <nasal>
        <load>
            var livery_update = aircraft.livery_update.new("Aircraft/747-400/Models");
        </load>

        <unload>
            livery_update.stop();
        </unload>
    </nasal> 

<animation> 
 <type>material</type> 
 <property-base>sim/model/livery</property-base> 
 <texture-prop>texture</texture-prop> 
 <texture>BOE.png</texture> 
</animation>

<!-- NOSE CONE -->
<animation>
 <type>translate</type>
 <object-name>nosecone</object-name>
 <property>controls/doors/nosecone/position-norm</property>
 <factor>0.20</factor>
 <center>
  <x-m>-28.701</x-m>
  <y-m> -1.002</y-m>
  <z-m>  1.075</z-m>
 </center>
 <axis>
  <x>1</x>
  <y>-1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>nosecone</object-name>
 <property>controls/doors/nosecone/position-norm</property>
 <factor>90</factor>
 <center>
  <x-m>-28.701</x-m>
  <y-m> -1.002</y-m>
  <z-m>  1.075</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>0</y>
  <z>1</z>
 </axis>
</animation>

<!-- DOORS-->

<!-- UPPERDECK LEFT -->

<animation>
 <type>rotate</type>
 <object-name>door_upperdeck_left</object-name>
 <property>controls/doors/upperdeckleft/position-norm</property>
 <factor>-55</factor>
 <axis>
  <x1-m>-15.022</x1-m>
  <y1-m> -1.607</y1-m>
  <z1-m>  4.476</z1-m>
  <x2-m>-14.158</x2-m>
  <y2-m> -1.610</y2-m>
  <z2-m>  4.476</z2-m>
 </axis>
</animation>

