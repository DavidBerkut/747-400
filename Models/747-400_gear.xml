  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>GearWLUpper</object-name>
  <condition>
   <or>
    <greater-than>
     <property>gear/gear[1]/position-norm</property>
     <value>0.9</value>
    </greater-than>
    <property>systems/seat/gear-well</property>
   </or>
  </condition>
 </animation>

<!-- basic movements -->

