     <property>gear/gear[1]/position-norm</property>
     <value>0.0</value>
    </greater-than>
    <property>systems/seat/gear-well</property>
   </or>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>GearWLSupport</object-name>
  <object-name>GearWLLower</object-name>
