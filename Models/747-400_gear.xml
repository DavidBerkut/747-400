  <type>rotate</type>
  <object-name>GearWingLeft</object-name>
  <property>gear/gear[1]/position-norm</property>
  <interpolation>
    <entry>
     <ind>0.0</ind>
     <dep>86</dep>
    </entry>
    <entry>
     <ind>0.3</ind>
     <dep>86</dep>
    </entry>
    <entry>
     <ind>1.0</ind>
     <dep>0</dep>
    </entry>
  </interpolation>
  <center>
   <x-m>2.095</x-m>
   <y-m>-5.51</y-m>
   <z-m>-1.474</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>GearWingLeftWheels</object-name>
  <property>gear/gear[1]/compression-norm</property>
  <factor>0.6</factor>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

