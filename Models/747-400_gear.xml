<PropertyList>

 <path>747-400_gear.ac</path>
 <texture-path>Textures/Livery/KLM/</texture-path>
 <offsets>
  <x-m>0</x-m>
  <y-m>0</y-m>
  <z-m>0</z-m>
 </offsets>

<!-- NOSE -->

 <animation>
  <name>GearNoseWheels</name>
  <object-name>Mesh49 Wheel2 Model</object-name>
  <object-name>Mesh50 Wheel2 Model</object-name>
  <object-name>Mesh81 Wheel18 Model</object-name>
  <object-name>Mesh82 Wheel18 Model</object-name>
  <object-name>Mesh17 GearNose_002_2</object-name>
  <object-name>Mesh18 GearNose_003_2</object-name>
  <object-name>Mesh26 GearNoseTorq_2</object-name>
  <object-name>Mesh27 GearNoseTorq_2</object-name>
 </animation>

 <animation>
  <name>GearNose</name>
  <object-name>Mesh15 GearNose_000_2</object-name>
  <object-name>Mesh16 GearNose_001_2</object-name>
  <object-name>Mesh19 GearNoseFore_2</object-name>
  <object-name>Mesh20 GearNoseFore_2</object-name>
  <object-name>Mesh21 GearNoseLeft_2</object-name>
  <object-name>Mesh22 GearNoseLeft_2</object-name>
  <object-name>Mesh23 GearNoseMain_2</object-name>
  <object-name>Mesh24 GearNoseRigh_2</object-name>
  <object-name>Mesh25 GearNoseRigh_2</object-name>
  <object-name>GearNoseWheels</object-name>
 </animation>

 <animation>
  <type>select</type>
  <object-name>GearNose</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[1]/position-norm</property>
    <value>0.0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>GearNose</object-name>
  <property>gear/gear[0]/position-norm</property>
  <interpolation>
    <entry>
     <ind>0.0</ind>
     <dep>115</dep>
    </entry>
    <entry>
     <ind>0.1</ind>
     <dep>115</dep>
    </entry>
    <entry>
     <ind>0.9</ind>
     <dep>0</dep>
    </entry>
    <entry>
     <ind>1.0</ind>
     <dep>0</dep>
    </entry>
  </interpolation>
  <center>
   <x-m>-21.991</x-m>
   <y-m>0.0</y-m>
   <z-m>-2</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <condition>
   <less-than>
    <property>position/altitude-agl-ft</property>        <!-- weight sensor -->
    <value>30.0</value>
   </less-than>
  </condition>
  <object-name>GearNoseWheels</object-name>
  <property>gear/gear[0]/steering-norm</property>
  <factor>-70</factor>
  <center>
   <x-m>-21.991</x-m>
   <y-m>0.0</y-m>
   <z-m>-2</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>GearNoseWheels</object-name>
  <property>gear/gear[0]/compression-norm</property>
  <factor>0.3</factor>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

<!-- WING -->
<!-- LEFT -->

