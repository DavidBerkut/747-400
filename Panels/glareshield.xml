</animation>

<animation>
 <type>select</type>
 <object-name>caution_lit</object-name>
 <condition>
  <equals>
   <property>/instrumentation/annunciators/master-caution</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>caution_unlit</object-name>
 <condition>
  <not>
   <equals>
    <property>/instrumentation/annunciators/master-caution</property>
    <value>1</value>
   </equals>
  </not>
 </condition>
</animation>

