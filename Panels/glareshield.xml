<PropertyList>

<path>glareshield.ac</path>

<!-- PANEL LIGHTING -->

<animation>
 <type>material</type>
 <object-name>AFDS</object-name>
 <object-name>EFIS</object-name>
 <object-name>EICAS</object-name>
 <object-name>disengage</object-name>
 <object-name>VS_dn</object-name>
 <object-name>VS_up</object-name>
 <object-name>FD</object-name>
 <emission>
  <red>0.5</red>
  <green>0.45</green>
  <blue>0.2</blue>
  <factor-prop>controls/lighting/panel-norm</factor-prop>
 </emission>
</animation>

<!-- WARNING/CAUTION -->

<animation>
 <type>pick</type>
 <object-name>warning_lit</object-name>
 <object-name>warning_unlit</object-name>
 <action>
  <button>0</button>
  <repeatable>false</repeatable>  
  <binding>
   <command>property-toggle</command>
   <property>/instrumentation/annunciators/master-warning</property>
  </binding>
 </action>
</animation>

<animation>
 <type>select</type>
 <object-name>warning_lit</object-name>
 <condition>
  <equals>
   <property>/instrumentation/annunciators/master-warning</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>warning_unlit</object-name>
 <condition>
  <not>
   <equals>
    <property>/instrumentation/annunciators/master-warning</property>
    <value>1</value>
   </equals>
  </not>
 </condition>
</animation>

<animation>
 <type>pick</type>
 <object-name>caution_lit</object-name>
 <object-name>caution_unlit</object-name>
 <action>
  <button>0</button>
  <repeatable>false</repeatable>  
