<!-- VS -->

<animation>
 <type>pick</type>
 <object-name>VS_dn</object-name>
 <action>
  <button>0</button>
  <repeatable>true</repeatable>  
  <binding>
   <command>property-adjust</command>
   <property>/autopilot/settings/vertical-speed-fpm</property>
   <step>-100</step>
   <min>-8000</min>
   <max>6000</max>
  </binding>
 </action>
</animation>

