<PropertyList>

<path>pedestal.ac</path>

<!-- THROTTLE LEVER -->
<!-- ENGINE 1 -->
<animation>
<type>rotate</type>
<object-name>throttle_eng.1</object-name>
<object-name>reverser.1</object-name>
<property>/controls/engines/engine/throttle</property>
<interpolation>
 <entry>
     <ind>0</ind>
     <dep>15</dep>
    </entry>
 <entry>
     <ind>1</ind>
     <dep>-20</dep>
    </entry>
</interpolation>
<center>
<x-m>1.143</x-m>
<y-m>0</y-m>
<z-m>0.362</z-m>
</center>
<axis>
<x>0.0</x>
<y>1.0</y>
<z>0</z>
</axis>
</animation>

<animation>
<type>rotate</type>
<object-name>reverser.1</object-name>
<property>/controls/engines/engine/reverser</property>
<interpolation>
<entry>
     <ind>0</ind>
     <dep>0</dep>
    </entry>
<entry>
     <ind>1</ind>
     <dep>25</dep>
    </entry>
</interpolation>
<center>
<x-m>1.12</x-m>
<y-m>0</y-m>
<z-m>0.54</z-m>
</center>
<axis>
<x>0.0</x>
<y>1.0</y>
<z>0</z>
</axis>
</animation>

