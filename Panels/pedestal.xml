<!-- SPEEDBRAKE LEVER -->

 <animation>
  <type>pick</type>
  <object-name>speedbrake</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>  
   <binding>
    <command>property-toggle</command>
    <property>/controls/flight/speedbrake</property>
   </binding>
  </action>
 </animation>

<animation>
<type>rotate</type>
<object-name>speedbrake</object-name>
<property>/controls/flight/speedbrake</property>
<factor>58.2</factor>
<center>
<x-m>1.143</x-m>
<y-m>0</y-m>
<z-m>0.362</z-m>
</center>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
</animation>

