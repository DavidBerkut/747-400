<!-- PANEL LIGHTING -->
<animation>
 <type>material</type>
 <object-name>forward_panel</object-name>
 <emission>
  <red>1</red>
  <green>0.9</green>
  <blue>0.4</blue>
  <factor-prop>controls/lighting/panel</factor-prop>
 </emission>
</animation>

<!-- GEAR HANDLE -->
<animation>
 <type>rotate</type>
 <object-name>gear_handle</object-name>
 <property>controls/gear/gear-down</property>
 <factor>45</factor>
 <center>
  <x-m>0.531</x-m>
  <y-m>0</y-m>
  <z-m>0.578</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>pick</type>
 <object-name>gear_handle</object-name>
 <action>
  <button>0</button>
  <repeatable>false</repeatable>
  <binding>
   <command>property-toggle</command>
   <property>controls/gear/gear-down</property>
  </binding>
 </action>
</animation>

