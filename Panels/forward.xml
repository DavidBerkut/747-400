<!-- BUTTONS -->

  <animation>
  <type>pick</type>
  <object-name>Mesh8 Group6 glareshi_1</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>  
   <binding>
    <command>property-cycle</command>
    <property>/autopilot/locks/heading</property>
    <value type="string">wing-leveler</value>
    <value type="string"></value>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/autopilot/buttons/heading-wing-leveler</property>
    <value type="string">0</value>
    <value type="string">1</value>
   </binding>
  </action>
 </animation>

  <animation>
  <type>pick</type>
  <object-name>Mesh10 Group8 glaresh_1</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
            
    <command>property-cycle</command>
  
      <property>/autopilot/locks/altitude</property>
      <value type="string">vertical-speed-hold</value>
      <value type="string"></value>

   </binding>  
   <binding>
    <command>property-toggle</command>
    <property>/autopilot/buttons/altitude-vs-hold</property>
    <value type="string">0</value>
    <value type="string">1</value>
   </binding>
  </action>
 </animation>

