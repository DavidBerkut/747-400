<PropertyList>

<path>yokes.ac</path>

<!-- CAPTAIN -->
 
<animation>
 <type>rotate</type>
 <object-name>left_column</object-name>
 <object-name>left_yoke</object-name>
 <property>controls/flight/elevator</property>
 <factor>10</factor>
 <center>
  <x-m> 1.10</x-m>
  <y-m>-0.533</y-m>
  <z-m>-0.025</z-m>
 </center>
 <axis>
  <x>0.0</x>
  <y>-1.0</y>
  <z>0.0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>left_yoke</object-name>
 <property>controls/flight/aileron</property>
 <factor>30</factor>
 <center>
  <x-m> 1.047</x-m>
  <y-m>-0.535</y-m>
  <z-m> 0.727</z-m>
 </center>
 <axis>
  <x>-1.0</x>
  <y>0.0</y>
  <z>0.0</z>
 </axis>
</animation>

