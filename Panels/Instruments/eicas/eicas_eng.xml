<!-- GEAR -->

<animation>
 <type>select</type>
 <object-name>Gear.Annun</object-name>
 <condition>
  <and>
   <greater-than>
      <property>/gear/gear[0]/position-norm</property>
    <value>0.0</value>
   </greater-than>
   <greater-than>
      <property>/gear/gear[1]/position-norm</property>
    <value>0.0</value>
   </greater-than>
   <greater-than>
      <property>/gear/gear[2]/position-norm</property>
    <value>0.0</value>
   </greater-than>
   <greater-than>
      <property>/gear/gear[3]/position-norm</property>
    <value>0.0</value>
   </greater-than>
   <greater-than>
      <property>/gear/gear[4]/position-norm</property>
    <value>0.0</value>
   </greater-than>
  </and>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>GearAnnun.ptr1</object-name>
 <condition>
  <and>
   <greater-than>
      <property>/gear/gear[0]/position-norm</property>
    <value>0.0</value>
   </greater-than>
   <greater-than>
      <property>/gear/gear[1]/position-norm</property>
    <value>0.0</value>
   </greater-than>
   <greater-than>
      <property>/gear/gear[2]/position-norm</property>
    <value>0.0</value>
   </greater-than>
   <greater-than>
      <property>/gear/gear[3]/position-norm</property>
    <value>0.0</value>
   </greater-than>
   <greater-than>
      <property>/gear/gear[4]/position-norm</property>
    <value>0.0</value>
   </greater-than>
   <less-than>
      <property>/gear/gear[0]/position-norm</property>
    <value>1.0</value>
   </less-than>
   <less-than>
      <property>/gear/gear[1]/position-norm</property>
    <value>1.0</value>
   </less-than>
   <less-than>
      <property>/gear/gear[2]/position-norm</property>
    <value>1.0</value>
   </less-than>
   <less-than>
      <property>/gear/gear[3]/position-norm</property>
    <value>1.0</value>
   </less-than>
   <less-than>
      <property>/gear/gear[4]/position-norm</property>
    <value>1.0</value>
   </less-than>
  </and>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>GearAnnun.ptr2</object-name>
 <condition>
  <and>
   <equals>
      <property>/gear/gear[0]/position-norm</property>
    <value>1.0</value>
   </equals>
   <equals>
      <property>/gear/gear[1]/position-norm</property>
    <value>1.0</value>
   </equals>
   <equals>
      <property>/gear/gear[2]/position-norm</property>
    <value>1.0</value>
   </equals>
   <equals>
      <property>/gear/gear[3]/position-norm</property>
    <value>1.0</value>
   </equals>
   <equals>
      <property>/gear/gear[4]/position-norm</property>
    <value>1.0</value>
   </equals>
  </and>
 </condition>
</animation>

