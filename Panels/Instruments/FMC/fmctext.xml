<?xml version="1.0"?>


<PropertyList>
    <name>FMCtext</name>
    <w-base>256</w-base>
    <h-base>256</h-base>

    <layers>

        <layer>
            <name>SPD annun</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>7</point-size>
            <color>
                <red>0.0</red>
                <green>1.0</green>
                <blue>0.0</blue>
            </color>
            <chunks>
                <chunk>
                    <type>literal</type>
                    <text>TEST</text>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>0</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>0</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>SPD annun</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>7</point-size>
            <color>
                <red>0.0</red>
                <green>1.0</green>
                <blue>0.0</blue>
            </color>
            <chunks>
                <chunk>
                    <type>literal</type>
                    <text>PROGRESS</text>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-70</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>112</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>SPD annun</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>7</point-size>
            <color>
                <red>0.0</red>
                <green>1.0</green>
                <blue>0.0</blue>
            </color>
            <chunks>
                <chunk>
                    <type>literal</type>
                    <text>WIND</text>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-57</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>95</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>windhdg</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>7</point-size>
            <color>
                <red>0.0</red>
                <green>1.0</green>
                <blue>0.0</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>environment/wind-from-heading-deg</property>
                    <format>%3.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-55</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>80</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>SPD annun</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>7</point-size>
            <color>
                <red>0.0</red>
                <green>1.0</green>
                <blue>0.0</blue>
            </color>
            <chunks>
                <chunk>
                    <type>literal</type>
                    <text>/</text>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-55</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>80</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>windspd</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>7</point-size>
            <color>
                <red>0.0</red>
                <green>1.0</green>
                <blue>0.0</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>environment/wind-speed-kt</property>
                    <format>%3.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-45</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>80</offset>
                </transformation>
            </transformations>
        </layer>

</layers>

</PropertyList>
