<!-- PANEL LIGHTING -->
 <animation>
  <type>material</type>
  <object-name>Mesh35 forward_main_p</object-name>
  <emission>
    <red>1</red>
    <green>0.9</green>
    <blue>0.4</blue>
  <factor-prop>controls/lighting/panel</factor-prop>
  </emission>
 </animation>

<!-- ANIMATIONS -->
<!-- YOKES -->
<!-- CAPTAIN -->
 
 <animation>
  <type>rotate</type>
   <object-name>Mesh10 Group5 Group4 </object-name>
   <object-name>Mesh9 fowheel2 Group5</object-name>
   <property>controls/flight/elevator</property>
   <factor>10</factor>
   <center>
    <x-m>1.10</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.05</z-m>
   </center>
   <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
   </axis>
 </animation>

 <animation>
  <type>rotate</type>
   <object-name>Mesh9 fowheel2 Group5</object-name>
  <property>controls/flight/aileron</property>
  <factor>30</factor>
  <center>
   <x-m>-0.53</x-m>
   <y-m>-0.6</y-m>
   <z-m>0.73</z-m>
  </center>
  <axis>
   <x>-1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

<!-- FIRST OFFICER -->
 <animation>
  <type>rotate</type>
   <object-name>Mesh3 fowheel1 Group2</object-name>
   <object-name>Mesh4 Group2 Group1 c</object-name>
   <property>controls/flight/elevator</property>
   <factor>10</factor>
   <center>
    <x-m>1.10</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.05</z-m>
