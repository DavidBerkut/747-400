<!-- Electrical -->

<animation>
	<type>pick</type>
	<object-name>apu</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>  
		<binding>
			<command>property-adjust</command>
			<property>/controls/APU/off-start-run</property>
			<step>1</step>
			<min>0</min>
			<max>2</max>
		</binding>
	</action>
</animation>

<animation>
	<type>pick</type>
	<object-name>apu</object-name>
	<action>
		<button>1</button>
