<?xml version="1.0"?>
 
<PropertyList>

<fx>

  <gear>
   <name>gear</name>
   <mode>in-transit</mode>
   <path>Sounds/gear.wav</path>
   <property>/gear/gear[0]/position-norm</property>
   <pitch>
    <internal>dt_play</internal>
    <offset>1.0</offset>
    <factor>-0.001</factor>
   </pitch>
  </gear>

  <gear>
   <name>gear-lock</name>
   <path>Sounds/gear-lck.wav</path>
   <condition>
    <equals>
     <property>/gear/gear[0]/position-norm</property>
     <value>0</value>
    </equals>
   </condition>
  </gear>

  <gear>
   <name>gear-horn</name>
   <mode>looped</mode>
   <path>Sounds/gear-hrn.wav</path>
   <condition>
    <and>
     <equals>
      <property>/gear/gear[2]/wow</property>
      <value>0</value>
     </equals>
     <equals>
      <property>/controls/gear/gear-down</property>
      <value>0</value>
     </equals>
     <or>
      <and>
       <or>
        <equals>
         <property>/controls/engines/engine[0]/throttle</property>
         <value>0</value>
        </equals>
        <equals>
         <property>/controls/engines/engine[1]/throttle</property>
         <value>0</value>
        </equals>
        <equals>
         <property>/controls/engines/engine[2]/throttle</property>
         <value>0</value>
        </equals>
        <equals>
         <property>/controls/engines/engine[3]/throttle</property>
         <value>0</value>
        </equals>
       </or>
       <less-than>
        <property>/position/altitude-agl-ft</property>
        <value>800</value>
       </less-than>
      </and>
      <greater-than>
       <property>/controls/flight/flaps</property>
       <value>0.667</value>
      </greater-than>
     </or>
    </and>
   </condition>
  </gear>

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
     <property>/gear/gear[3]/wow</property>
     <property>/gear/gear[4]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/speed-down-fps</property>
    <factor>0.05</factor>
   </volume>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>

  <flap>
   <name>flaps</name>
   <mode>in-transit</mode>
   <path>Sounds/flaps.wav</path>
   <property>/surface-positions/flap-pos-norm</property>
   <volume>
    <factor>0.45</factor>
   </volume>
  </flap>

  <wind>
   <name>wind</name>
   <mode>looped</mode>
   <path>Sounds/wind-8khz.wav</path>
   <property>/velocities/airspeed-kt</property>
   <volume>
    <property>/position/altitude-ft</property>
    <factor>-0.000015</factor>
    <offset>1.0</offset>
    <min>0.1</min>
    <max>1.0</max>
   </volume>
   <volume>
     <property>/velocities/airspeed-kt</property>
     <factor>0.0015</factor>
     <min>0.03</min>
     <max>0.25</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
    <offset>1.25</offset>
   </pitch>
  </wind>

  <stall>
   <name>stall</name>
   <path>Sounds/stall.wav</path>
   <property>/sim/alarms/stall-warning</property>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <min>125.0</min>
   </volume>
  </stall>

  <click>
   <name>click1</name>
   <path>Sounds/click.wav</path>
   <condition>
    <property>sim/sound/click</property>
   </condition>
   <volume>
    <property>sim/sound/Cvolume</property>
    <factor>0.3</factor>
   </volume>
   <position>
    <x>5</x>
    <y>0</y>
    <z>0</z>
   </position>
   <reference-dist>2.5</reference-dist>
   <max-dist>5.0</max-dist>
  </click>
  
  <switch>
   <name>click on</name>
   <mode>once</mode>
   <path>Aircraft/747-400/Sounds/FastenSeatbelt.wav</path>
   <condition>
    <property>controls/switches/seatbelt-sign</property>
   </condition>
   <volume>
    <factor>1</factor>
   </volume>
  </switch>

  <switch>
   <name>click on2</name>
   <mode>once</mode>
   <path>Aircraft/747-400/Sounds/autopilot-disengage.wav</path>
   <condition>
    <not>
     <and>
      <property>sim/gui/dialogs/autopilot/altitude-active</property>
      <property>sim/gui/dialogs/autopilot/heading-active</property>
      <property>sim/gui/dialogs/autopilot/speed-active</property>
     </and>
	</not>
   </condition>
   <volume>
    <factor>1</factor>
   </volume>
  </switch>
  
  <!-- engines -->
   
  <engine>
   <name>engine1run</name>
   <path>Aircraft/747-400/Sounds/engine-run.wav</path>
   <mode>looped</mode>
   <condition>
    <greater-than>
     <property>/engines/engine[0]/n2</property>
	 <value>30</value>
	</greater-than>
   </condition>
   <volume>
    <property>/engines/engine[0]/n2</property>
    <type>ln</type>
    <factor>0.035</factor>
   </volume>
  </engine>
  
  <engine>
   <name>engine2run</name>
   <path>Aircraft/747-400/Sounds/engine-run.wav</path>
   <mode>looped</mode>
   <condition>
    <greater-than>
     <property>/engines/engine[1]/n2</property>
	 <value>30</value>
	</greater-than>
   </condition>
   <volume>
    <property>/engines/engine[1]/n2</property>
    <type>ln</type>
    <factor>0.035</factor>
   </volume>
  </engine>
  
  <engine>
   <name>engine3run</name>
   <path>Aircraft/747-400/Sounds/engine-run.wav</path>
   <mode>looped</mode>
   <condition>
    <greater-than>
     <property>/engines/engine[2]/n2</property>
	 <value>30</value>
	</greater-than>
   </condition>
   <volume>
    <property>/engines/engine[2]/n2</property>
    <type>ln</type>
    <factor>0.035</factor>
   </volume>
  </engine>
  
  <engine>
   <name>engine4run</name>
   <path>Aircraft/747-400/Sounds/engine-run.wav</path>
   <mode>looped</mode>
   <condition>
    <greater-than>
     <property>/engines/engine[3]/n2</property>
	 <value>30</value>
	</greater-than>
   </condition>
   <volume>
    <property>/engines/engine[3]/n2</property>
    <type>ln</type>
    <factor>0.035</factor>
   </volume>
  </engine>

<!-- custom GPWS-->

        <altitude1>
            <name>alt-10</name>
            <reference-dist>1.0</reference-dist>
            <max-dist>2.0</max-dist>
            <mode>once</mode>
            <path>Aircraft/747-400/Sounds/altitude-10.wav</path>
            <condition>
			<and>
				<less-than>
                    		<property>velocities/vertical-speed-fps</property>
                    		<value>0.0</value>
                  	</less-than>
				<less-than-equals>
                    		<property>position/altitude-agl-ft</property>
                    		<value>10</value>
                  	</less-than-equals>
			</and>
            </condition>
            <volume>
                <property>instrumentation/annunciators/gpws/volume</property>
            </volume>
            <position>
                <x>-5</x>
                <y>0</y>
                <z>0</z>
            </position>
        </altitude1>

