<!-- # JETTISON system for BOEING 747-400 #
     # by Gijs de Rooy			          # -->

<system name="jettison">
    
    <channel name="jettison">

        <kinematic name="propulsion/systems/tank/jettison-qty">
           <input>propulsion/systems/tank/jettison-switch</input>
           <traverse>
               <setting>
                   <position>0.0</position>
                   <time>0.0</time>
               </setting>
               <setting>
                   <position>88184</position>
                   <time>3600</time>
               </setting>
           </traverse>
        </kinematic>

    </channel>

</system>